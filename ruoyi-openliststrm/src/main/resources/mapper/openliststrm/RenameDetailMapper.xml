<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.openliststrm.mapper.RenameDetailMapper">
    
    <resultMap type="RenameDetail" id="RenameDetailResult">
        <result property="id"    column="id"    />
        <result property="originalPath"    column="originalPath"    />
        <result property="originalName"    column="originalName"    />
        <result property="newPath"    column="newPath"    />
        <result property="newName"    column="newName"    />
        <result property="mediaType"    column="media_type"    />
        <result property="title"    column="title"    />
        <result property="year"    column="year"    />
        <result property="season"    column="season"    />
        <result property="episode"    column="episode"    />
        <result property="tmdbId"    column="tmdbId"    />
        <result property="resolution"    column="resolution"    />
        <result property="videoCodec"    column="videoCodec"    />
        <result property="audioCodec"    column="audioCodec"    />
        <result property="source"    column="source"    />
        <result property="releaseGroup"    column="releaseGroup"    />
        <result property="status"    column="status"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectRenameDetailVo">
        select id, originalPath, originalName, newPath, newName, media_type, title, year, season, episode, tmdbId, resolution, videoCodec, audioCodec, source, releaseGroup, status, create_time, update_time from rename_detail
    </sql>

    <select id="selectRenameDetailList" parameterType="RenameDetail" resultMap="RenameDetailResult">
        <include refid="selectRenameDetailVo"/>
        <where>  
            <if test="originalPath != null  and originalPath != ''"> and originalPath like concat('%', #{originalPath}, '%')</if>
            <if test="originalName != null  and originalName != ''"> and originalName like concat('%', #{originalName}, '%')</if>
            <if test="newPath != null  and newPath != ''"> and newPath like concat('%', #{newPath}, '%')</if>
            <if test="newName != null  and newName != ''"> and newName like concat('%', #{newName}, '%')</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectRenameDetailById" parameterType="Integer" resultMap="RenameDetailResult">
        <include refid="selectRenameDetailVo"/>
        where id = #{id}
    </select>

    <insert id="insertRenameDetail" parameterType="RenameDetail" useGeneratedKeys="true" keyProperty="id">
        insert into rename_detail
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="originalPath != null and originalPath != ''">originalPath,</if>
            <if test="originalName != null and originalName != ''">originalName,</if>
            <if test="newPath != null">newPath,</if>
            <if test="newName != null">newName,</if>
            <if test="mediaType != null">media_type,</if>
            <if test="title != null">title,</if>
            <if test="year != null">year,</if>
            <if test="season != null">season,</if>
            <if test="episode != null">episode,</if>
            <if test="tmdbId != null">tmdbId,</if>
            <if test="resolution != null">resolution,</if>
            <if test="videoCodec != null">videoCodec,</if>
            <if test="audioCodec != null">audioCodec,</if>
            <if test="source != null">source,</if>
            <if test="releaseGroup != null">releaseGroup,</if>
            <if test="status != null">status,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="originalPath != null and originalPath != ''">#{originalPath},</if>
            <if test="originalName != null and originalName != ''">#{originalName},</if>
            <if test="newPath != null">#{newPath},</if>
            <if test="newName != null">#{newName},</if>
            <if test="mediaType != null">#{mediaType},</if>
            <if test="title != null">#{title},</if>
            <if test="year != null">#{year},</if>
            <if test="season != null">#{season},</if>
            <if test="episode != null">#{episode},</if>
            <if test="tmdbId != null">#{tmdbId},</if>
            <if test="resolution != null">#{resolution},</if>
            <if test="videoCodec != null">#{videoCodec},</if>
            <if test="audioCodec != null">#{audioCodec},</if>
            <if test="source != null">#{source},</if>
            <if test="releaseGroup != null">#{releaseGroup},</if>
            <if test="status != null">#{status},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateRenameDetail" parameterType="RenameDetail">
        update rename_detail
        <trim prefix="SET" suffixOverrides=",">
            <if test="originalPath != null and originalPath != ''">originalPath = #{originalPath},</if>
            <if test="originalName != null and originalName != ''">originalName = #{originalName},</if>
            <if test="newPath != null">newPath = #{newPath},</if>
            <if test="newName != null">newName = #{newName},</if>
            <if test="mediaType != null">media_type = #{mediaType},</if>
            <if test="title != null">title = #{title},</if>
            <if test="year != null">year = #{year},</if>
            <if test="season != null">season = #{season},</if>
            <if test="episode != null">episode = #{episode},</if>
            <if test="tmdbId != null">tmdbId = #{tmdbId},</if>
            <if test="resolution != null">resolution = #{resolution},</if>
            <if test="videoCodec != null">videoCodec = #{videoCodec},</if>
            <if test="audioCodec != null">audioCodec = #{audioCodec},</if>
            <if test="source != null">source = #{source},</if>
            <if test="releaseGroup != null">releaseGroup = #{releaseGroup},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteRenameDetailById" parameterType="Integer">
        delete from rename_detail where id = #{id}
    </delete>

    <delete id="deleteRenameDetailByIds" parameterType="String">
        delete from rename_detail where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>